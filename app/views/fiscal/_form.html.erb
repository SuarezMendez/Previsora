          <%= form.text_field :fail_previ_cents, onkeyup:"formatting_number(this)", class:"form-control" %>
          <span class="input-group-addon" data-toggle="tooltip" title="breve descripción" data-placement="auto top">
            <i class="glyphicon glyphicon-question-sign"></i>
          </span>
        </div>

        <%= form.label "Valor pagado Previsora" %>
        <div class="input-group">
          <%= form.text_field :payed_value_cents, onkeyup:"formatting_number(this)", class:"form-control" %>
          <span class="input-group-addon" data-toggle="tooltip" title="breve descripción" data-placement="auto top">
            <i class="glyphicon glyphicon-question-sign"></i>
          </span>
        </div>

        <%= form.label "Fecha de consignación" %>
        <div class="input-group">
          <%= form.date_field :payment_date, class:"form-control" %>
          <span class="input-group-addon" data-toggle="tooltip" title="<%= t('processes.fiscal.payment_date_d') %>" data-placement="auto top">
            <i class="glyphicon glyphicon-question-sign"></i>
          </span>
        </div>

        <%= form.label "¿Tiene número de radicado de coactivo?" %>
        <div class="input-group">
          <div class="styled-select">
            <select id="coactive" class="form-control">
              <option value="Si">Si</option>
              <option value="No">No</option>
            </select>
          </div>
          <span class="input-group-addon" data-toggle="tooltip" title="<%= t('processes.fiscal.coactive_d') %>" data-placement="auto top">
            <i class="glyphicon glyphicon-question-sign"></i>
          </span>
        </div>

        <%= form.label "Número de radicación de coactivo" %>
        <div class="input-group">
          <%= form.text_field :coactive_radicate, id:"coactive_radicate", pattern:"[A-Z0-9\- ]*", title: t(:pattern), onclick:"input_pendiente_rule(this)", onblur:"input_empty_pendiente_rule(this)", class:"form-control" %>
          <span class="input-group-addon" data-toggle="tooltip" title="<%= t('processes.fiscal.coactive_radicate_d') %>" data-placement="auto top">
            <i class="glyphicon glyphicon-question-sign"></i>
          </span>
        </div>

        <%= form.label "Valor coactivo" %>
        <div class="input-group">
          <%= form.text_field :coactive_value_cents, id:"coactive_value_cents", onkeyup:"formatting_number(this)", class:"form-control" %>
          <span class="input-group-addon" data-toggle="tooltip" title="breve descripción" data-placement="auto top">
            <i class="glyphicon glyphicon-question-sign"></i>
          </span>
        </div>

        <%= form.label "Valor embargo" %>
        <div class="input-group">
          <%= form.text_field :garnish_value_cents, onkeyup:"formatting_number(this)", class:"form-control" %>
          <span class="input-group-addon" data-toggle="tooltip" title="breve descripción" data-placement="auto top">
            <i class="glyphicon glyphicon-question-sign"></i>
          </span>
        </div>

        <%= form.label "Última actuación" %>
        <div class="input-group">
          <div class="styled-select">
            <%= form.collection_select(:last_performance, LastPerformance.all, :id, :name, {}, {class:"form-control", id:"lastPerformance"}) %>
          </div>
          <span class="input-group-addon" data-toggle="tooltip" title="<%= t('processes.fiscal.last_performance_id_d') %>" data-placement="auto top">
            <i class="glyphicon glyphicon-question-sign"></i>
          </span>
        </div>

        <%= form.label "Fecha última actuación" %>
        <div class="input-group">
          <%= form.date_field :last_performance_date, class:"form-control",  id:"lastPerformanceDate" %>
          <span class="input-group-addon" data-toggle="tooltip" title="<%= t('processes.fiscal.last_performance_date_d') %>" data-placement="auto top">
            <i class="glyphicon glyphicon-question-sign"></i>
          </span>
        </div>

        <%= form.label "Reserva depurada" %>
        <div class="input-group">
          <div class="styled-select">
            <%= form.collection_select(:reserved_released, ReservedReleased.all, :id, :name, {}, {class:"form-control", id:"reservedReleased"}) %>
          </div>
          <span class="input-group-addon" data-toggle="tooltip" title="<%= t('processes.fiscal.reserved_released_id_d') %>" data-placement="auto top">
            <i class="glyphicon glyphicon-question-sign"></i>
          </span>
        </div>

        <hr>

      </div>
    </div>
  </div>

  <div class="p-30">
    <%= link_to 'Volver', lawyer_new_path, class:"btn btn-default" %>
    <%= form.submit "Guardar", class:"btn btn-default floatRight" %>
  </div>

<% end %>
