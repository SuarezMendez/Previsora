          <div class="styled-select">
            <%= form.collection_select(:litigation_source, LitigationSource.all, :id, :name, {}, {id:"litigationSource", class:"form-control"}) %>
          </div>
          <span class="input-group-addon" data-toggle="tooltip" title="<%= t('processes.fiscal.litigation_source_id_d') %>" data-placement="auto top">
            <i class="glyphicon glyphicon-question-sign"></i>
          </span>
        </div>

        <%= form.label "Tipo de reaseguro" %>
        <div class="input-group">
          <div class="styled-select">
            <%= form.collection_select(:reinsurance_type, ReinsuranceType.all, :id, :name, {}, {class:"form-control"}) %>
