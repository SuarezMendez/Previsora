          <span class="input-group-addon" data-toggle="tooltip" title="<%= t('processes.fiscal.sinisters_d') %>" data-placement="auto top">
            <i class="glyphicon glyphicon-question-sign"></i>
          </span>
        </div>

        <hr>

        <%= form.label "Tipo moneda" %>
        <div class="input-group">
          <div class="styled-select">
            <%= form.collection_select(:money_type, MoneyType.all, :id, :name, {}, {class:"form-control", id:"moneyType"}) %>
          </div>
          <span class="input-group-addon" data-toggle="tooltip" title="<%= t('processes.fiscal.money_type_id_d') %>" data-placement="auto top">
            <i class="glyphicon glyphicon-question-sign"></i>
          </span>
        </div>
        <%= form.label "Valor del dólar" %>
        <div class="input-group">
          <%= form.text_field :dolar_value_cents, id:"dolarValueCents", onkeyup:"formatting_number(this)", class:"form-control" %>
          <span class="input-group-addon" data-toggle="tooltip" title="breve descripción" data-placement="auto top">
            <i class="glyphicon glyphicon-question-sign"></i>
          </span>
        </div>

        <%= form.label "Reserva o provisión de indemnización" %>
        <div class="input-group">
          <%= form.number_field :reserve_cents, class:"form-control" %>
          <span class="input-group-addon" data-toggle="tooltip" title="<%= t('processes.fiscal.reserve_cents_d') %>" data-placement="auto top">
            <i class="glyphicon glyphicon-question-sign"></i>
          </span>
        </div>

        <%= form.label "Reserva honorarios" %>
        <div class="input-group">
          <%= form.number_field :reserved_fees_cents, class:"form-control" %>
